<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="echoverse.user.mapper.UserMapper">
<!--    UserPo getUserByEmail(String email);-->
    <select id="getUserByEmail" resultType="echoverse.model.po.UserPo">
        select * from echouser where email=#{email} and status=0 and is_delete=0
    </select>
</mapper>